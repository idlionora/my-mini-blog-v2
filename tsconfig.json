{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@pages/*": ["src/pages/*"],
			"@styles/*": ["src/styles/*"],
			"@scripts/*": ["src/scripts/*"],
			"@data/*": ["src/data/*"]
		},
		"plugins": [
			{
				"name": "@astrojs/ts-plugin"
			}
		]
  }
}
